	xsi:schemaLocation="http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
   http://www.springframework.org/schema/context
   http://www.springframework.org/schema/context/spring-context-3.0.xsd">

	<context:annotation-config></context:annotation-config>

	<bean name="cources" class="master.SeleniumExpress.Cources">
		<property name="courseId" value="01" />
		<property name="courseName" value="Spring" />
	</bean>

	<bean name="student" class="master.SeleniumExpress.Student">
		<property name="studentId" value="Stud01" />
		<property name="studentName" value="Chitradeep Sarkar" />
	</bean>


</beans>